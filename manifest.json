{
    "manifest_version": 3,
    "name": "NoteSaver",
    "version": "1.0",
    "description": "Save notes with ease!",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["storage"]
  }
  